default:
	echo 'Hello, world!'
lint:
        golangci-lint run ./...
test:
        go test -count=1 -coverprofile=coverage.out ./...
test-no-cover:
        go test -count=1 ./...
test-race:
        go test -count=1 -race ./...
fmt:
        @go fmt ./...
cover:
        @go tool cover -html=coverage.out
